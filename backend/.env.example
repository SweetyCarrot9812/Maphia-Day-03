# MongoDB 연결 정보
MONGODB_URI=mongodb://localhost:27017/haneultone
# 또는 MongoDB Atlas 사용시:
# MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/haneultone

# JWT 비밀키 (실제 운영시에는 강력한 랜덤 키 사용)
JWT_SECRET=your-super-secret-jwt-key-here

# 서버 포트
PORT=3000

# 환경 (development, production)
NODE_ENV=development

# CORS 허용 도메인
CORS_ORIGIN=http://localhost:*